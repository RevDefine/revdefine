{
    "extends": "@quasar/app/tsconfig-preset",
    "compilerOptions": {
        "allowJs": true,
        "sourceMap": true,
        "target": "es6",
        "strict": true,
        "experimentalDecorators": true,
        "module": "esnext",
        "moduleResolution": "node",
        "baseUrl": ".",
        "types": [
            "quasar",
            "node"
        ]
    },
    "exclude": [
        "node_modules",
        "grpc",
        "dist",
        "./babel.config.js",
        "./quasar.conf.js",
        "pb",
        "quasar"
    ]
    // "include": ["src/rnode-grpc-gen/js/rnode-grpc-js.d.ts"]
}